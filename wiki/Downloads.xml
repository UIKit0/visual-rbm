	<script type="text/javascript" language="javascript"> 
		function AddReleaseEntry(FileName, Description, Version)
		{
			table = document.getElementById("releases");
			tbody = table.children[0];
			tr = document.createElement("tr");
			
			// file link
			file_td = document.createElement("td");
			file_td_a = document.createElement("a");
			file_td_a.setAttribute("href", "http://visual-rbm.googlecode.com/svn/trunk/release/" + FileName);
			file_td_a.setAttribute("onclick", "_gaq.push(['_trackEvent','Releases', 'Download', FileName + '-' + Version])");
			file_td_a.innerHTML = FileName;

			file_td.appendChild(file_td_a);
			tr.appendChild(file_td);
			
			// description
			desc_td = document.createElement("td");
			desc_td.innerHTML = Description;

			tr.appendChild(desc_td);

			// version
			vers_td = document.createElement("td");
			vers_td.innerHTML = Version;
			
			tr.appendChild(vers_td);
			
			// History
			hist_td = document.createElement("td");
			hist_td_a = document.createElement("a");
			hist_td_a.setAttribute("href", "http://code.google.com/p/visual-rbm/source/list?path=/trunk/release/" + FileName);
			hist_td_a.innerHTML = "&hellip;";
			
			hist_td.appendChild(hist_td_a);
			tr.appendChild(hist_td);
			
			tbody.appendChild(tr);
		}
		
		AddReleaseEntry("VisualRBM.zip", "The latest VisualRBM client", "r58");
		AddReleaseEntry("Tools.zip", "A set of tools for manipulating IDX data files and training RBMs from the command-line", "r58");
		
	</script>
